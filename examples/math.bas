' Basic math operations demo
LET X = 10
LET Y = 2

' Basic arithmetic
PRINT "X = " + X
PRINT "Y = " + Y
PRINT "X + Y = " + (X + Y)
PRINT "X - Y = " + (X - Y)
PRINT "X * Y = " + (X * Y)
PRINT "X / Y = " + (X / Y)

' Expressions with parentheses
PRINT "(X + Y) * 2 = " + ((X + Y) * 2)
PRINT "X + Y * 2 = " + (X + Y * 2)
PRINT "X + Y / 2 = " + (X + Y / 2)
PRINT "X * Y + 2 = " + (X * Y + 2)
PRINT "X * (Y + 2) = " + (X * (Y + 2))
PRINT "X / (Y + 2) = " + (X / (Y + 2))
PRINT "X / Y + 2 = " + (X / Y + 2)
PRINT "(X - Y) * 2 = " + ((X - Y) * 2)
PRINT "X - Y * 2 = " + (X - Y * 2)
PRINT "X - Y / 2 = " + (X - Y / 2)
PRINT "X / Y - 2 = " + (X / Y - 2)
PRINT "X * (Y - 2) = " + (X * (Y - 2))
